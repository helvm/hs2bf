pr %exec/sc
    alloc cont
    while sc
        -- run GC before executing SC
        alloc sct
        copy sc sct
        val sct -1
        while sct
            clear sct
            inline #gc
        delete sct
        -- execute SC
        dispatch sc
            1
            3
                inline !Bar
            4
                inline !Halt
            5
                inline !Output
            2
                inline !main
            6
                inline !main_
            7
                inline !seq
            8
                inline !seq_
        val cont 1
    while cont
        val sc 1
        val cont -1
    delete cont

